<p id="notice"><%= notice %></p>

<h1>Submissions</h1>

<table>
  <thead>
    <tr>
      <th>Full name</th>
      <th>Email</th>
      <th>Project repo</th>
      <th>Project url</th>
      <th>Oop best practices</th>
      <th>Modular dev</th>
      <th>Workflow understanding</th>
      <th>Testing</th>
      <th>Database knowledge</th>
      <th>Debugging</th>
      <th>Problem solving</th>
      <th>Javascript</th>
      <th>Html</th>
      <th>Css</th>
      <th>Teamwork</th>
      <th>Self motivation</th>
      <th>Communication skils</th>
      <th>Energy level</th>
      <th>Intelligence</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td><%= submission.full_name %></td>
        <td><%= submission.email %></td>
        <td><%= submission.project_repo %></td>
        <td><%= submission.project_url %></td>
        <td><%= submission.oop_best_practices %></td>
        <td><%= submission.modular_dev %></td>
        <td><%= submission.workflow_understanding %></td>
        <td><%= submission.testing %></td>
        <td><%= submission.database_knowledge %></td>
        <td><%= submission.debugging %></td>
        <td><%= submission.problem_solving %></td>
        <td><%= submission.javascript %></td>
        <td><%= submission.html %></td>
        <td><%= submission.css %></td>
        <td><%= submission.teamwork %></td>
        <td><%= submission.self_motivation %></td>
        <td><%= submission.communication_skils %></td>
        <td><%= submission.energy_level %></td>
        <td><%= submission.intelligence %></td>
        <td><%= link_to 'Show', submission %></td>
        <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
        <td><%= link_to 'Destroy', submission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submission', new_submission_path %>
